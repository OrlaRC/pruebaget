{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "rewrites": [
    {
      "source": "/auth/callback/:path*",
      "destination": "/src/index.html"
    },
    {
      "source": "/info-auto/:path*",
      "destination": "/src/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/src/index.html"
    }
  ]
}
