{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/callback(?:\\?.*)?$",
      "dest": "/src/callback.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}
