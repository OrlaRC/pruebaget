{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/info-auto",
      "dest": "/src/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}
